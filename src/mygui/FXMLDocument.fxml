<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mygui.FXMLDocumentController">
   <children>
      <HBox layoutY="-1.0" prefHeight="279.0" prefWidth="803.0">
         <children>
            <VBox prefHeight="300.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Checkbox Objects">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <CheckBox fx:id="chbxPepperoni" mnemonicParsing="false" text="Pepperoni" />
                  <CheckBox fx:id="chbxPineApple" mnemonicParsing="false" text="Pineapple" />
                  <CheckBox fx:id="chbxBacon" mnemonicParsing="false" text="Bacon" />
                  <Button mnemonicParsing="false" onAction="#PizzaOrderButtonPushed" text="Order Pizza" />
                  <Label fx:id="lblPizzaOrder" alignment="TOP_LEFT" prefHeight="94.0" prefWidth="201.0" text="Label" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <VBox prefHeight="300.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Choicebox Object">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <ChoiceBox fx:id="choiceBox" prefWidth="150.0" />
                  <Button mnemonicParsing="false" onAction="#choiceBoxButtonPushed" text="Favourite Fruit" />
                  <Label fx:id="lblChoiceBox" text="Label" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Combobox Object">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="myComboBox" onAction="#comboBoxChange" prefWidth="150.0" />
                  <Label fx:id="lblComboBox" text="Label" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Radiobutton Objects">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <RadioButton fx:id="phpRadioButton" mnemonicParsing="false" onAction="#radioButtonChanged" text="PHP" />
                  <RadioButton fx:id="javaRadioButton" mnemonicParsing="false" onAction="#radioButtonChanged" text="Java" />
                  <RadioButton fx:id="c1RadioButton" mnemonicParsing="false" onAction="#radioButtonChanged" text="C#" />
                  <RadioButton fx:id="c2RadioButton" mnemonicParsing="false" onAction="#radioButtonChanged" text="C++" />
                  <Label fx:id="lblradioButton" text="Label" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
